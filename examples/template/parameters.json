{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "github-action"
        },
        "location": {
            "value": "westeurope"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "whiteduck/sample-mvc"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "0.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "8080",
                    "protocol": "TCP"
                }
            ]
        }
    }
}
